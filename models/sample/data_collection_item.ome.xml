<?xml version="1.0" encoding="UTF-8"?>

<types>
  <!-- A DataCollectionItem is a DataSample associated to a Study -->
  <type id="ome.model.vl.DataCollectionItem" superclass="ome.model.vl.Result">
    <properties>
      <required name="dataSample" type="ome.model.vl.DataSample"/>
      <required name="dataSet" type="ome.model.vl.DataCollection"/>
      <!-- Multi-field unique key -->
      <!-- dataCollUK = dataSample_dataSet -->
      <required name="dataCollUK" type="string" unique="true"/>
    </properties>
  </type>
</types>
