<?xml version="1.0" encoding="UTF-8"?>

<types>
  <type id="ome.model.vl.ContainerSlot" superclass="ome.model.vl.Result">
    <properties>
      <optional name="label" type="string"/>
      <required name="sample" type="ome.model.vl.BioSample"/>
      <required name="container" type="ome.model.vl.SamplesContainer"/>
      <required name="slotPosition" type="int"/>
      <!-- Multi-field unique key-->
      <!-- contSlotUK = container_slotPosition -->
      <required name="contSlotUK" type="string" unique="true"/>
      <!-- contLabelUK = container_label -->
      <optional name="contLabelUK" type="string" unique="true"/>
    </properties>
  </type>
</types>
